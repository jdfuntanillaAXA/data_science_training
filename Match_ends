from pandas import Series
ser1 = Series(["aaa","aab","aba","baa","bab"])
ser2 = Series(["dad","mom","bob","cat"])
def match_ends(words):
  count = 0
  for word in words:
    if len(word)>=2 and word[0] == word[-1]:
      count +=1
   return count
 match_ends(ser1)
 match_ends(ser2)
 match_ends(ser3)
